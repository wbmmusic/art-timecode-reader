{
    "name": "art-timecode-reader",
    "version": "0.0.5",
    "author": "Marece Williams",
    "description": "ArtNet Timecode Reader",
    "main": ".vite/build/main.js",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.4",
        "@mui/material": "^7.3.4",
        "electron-updater": "^6.6.2",
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    },
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "echo \"No linting configured\"",
        "backup": "git add . && git commit -m %NPM_PACKAGE_VERSION% && git push origin main",
        "refresh": "pnpm upgrade --latest"
    },
    "config": {
        "forge": {
            "packagerConfig": {
                "name": "ArtTimecode Reader",
                "executableName": "art-timecode-reader",
                "icon": "./src/assets/icon",
                "afterCopy": [
                    "./notarize.js"
                ]
            },
            "rebuildConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "art_timecode_reader",
                        "certificateSubjectName": "WBM Tek (Mareci, William)"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-dmg",
                    "config": {
                        "name": "ArtTimecode Reader",
                        "icon": "./src/assets/icon.icns"
                    }
                }
            ],
            "plugins": [
                {
                    "name": "@electron-forge/plugin-vite",
                    "config": {
                        "build": [
                            {
                                "entry": "src/main.js",
                                "config": "vite.main.config.js"
                            },
                            {
                                "entry": "src/preload.js",
                                "config": "vite.preload.config.js"
                            }
                        ],
                        "renderer": [
                            {
                                "name": "main_window",
                                "config": "vite.renderer.config.js"
                            }
                        ]
                    }
                }
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wbmmusic/art-timecode-reader.git"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.5.0",
        "@electron-forge/maker-deb": "^7.5.0",
        "@electron-forge/maker-dmg": "^7.5.0",
        "@electron-forge/maker-rpm": "^7.5.0",
        "@electron-forge/maker-squirrel": "^7.5.0",
        "@electron-forge/maker-zip": "^7.5.0",
        "@electron-forge/plugin-vite": "^7.5.0",
        "@electron/notarize": "^3.1.0",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.1",
        "@vitejs/plugin-react": "^5.0.4",
        "electron": "^38.2.1",
        "rollup-plugin-copy": "^3.5.0",
        "vite": "^7.1.9"
    },
    "packageManager": "pnpm@9.2.0+sha512.98a80fd11c2e7096747762304106432b3ddc67dcf54b5a8c01c93f68a2cd5e05e6821849522a06fb76284d41a2660d5e334f2ee3bbf29183bf2e739b1dafa771"
}